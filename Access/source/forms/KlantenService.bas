Private Sub btnSearch_Click()
    Dim SearchQuery As String
    Dim placeAND As Boolean
    placeAND = False
    
    If (Not IsNull(Me.txtPostalcode) And Not (Me.txtPostalcode.Value = "")) Then
        SearchQuery = SearchQuery & "[postalcode] LIKE '%" & Me.txtPostalcode.Value & "%'"
        placeAND = True
    End If
    
    If (Not IsNull(Me.txtPostalcode) And Not (Me.txtAdressNumber.Value = "")) Then
        If (placeAND) Then
            SearchQuery = SearchQuery & " AND "
        End If
        SearchQuery = SearchQuery & "[adress_number] = " & Me.txtAdressNumber.Value
        placeAND = True
    End If
        
    MsgBox "SELECT firstname + ' ' + lastname FROM users WHERE " & SearchQuery & " "
    Me.lstUsers.Requery
    
    
End Sub

