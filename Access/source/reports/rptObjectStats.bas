Option Compare Database

Private Sub Report_Load()
    Dim db As Database
    Dim rs As DAO.Recordset ''Requires reference to Microsoft DAO x.x Library
    Dim sSQL As String
    
    'Query that calculates the amount of objects
    sSQL = "SELECT COUNT(*) AS counted FROM objects"

    Set db = CurrentDb
    Set rs = db.OpenRecordset(sSQL)
    
     If rs.RecordCount > 0 Then
       Me.txtObjectAmount = rs!counted
    Else
       Me.txtObjectAmount = "N/A"
    End If
    
    'Query that calculates the amount of lots that are currently open
    sSQL = "SELECT COUNT(*) AS counted FROM objects WHERE buyer IS NULL"
    
    Set rs = db.OpenRecordset(sSQL)
    
    If rs.RecordCount > 0 Then
       Me.txtObjectBeingSoldAmount = rs!counted
    Else
       Me.txtObjectBeingSoldAmount = "N/A"
    End If
    
    'Query that calculates the amount of objects that were sold
    sSQL = "SELECT COUNT(*) AS counted FROM objects WHERE buyer IS NOT NULL"
    
    Set rs = db.OpenRecordset(sSQL)
    
    If rs.RecordCount > 0 Then
       Me.txtObjectSoldAmount = rs!counted
    Else
       Me.txtObjectSoldAmount = "N/A"
    End If
    
    'Query that calculates the amount of objects that were not sold
    sSQL = "SELECT COUNT(*) AS counted FROM objects WHERE buyer IS NOT NULL AND end_moment < DATE()"
    
    Set rs = db.OpenRecordset(sSQL)
    
    If rs.RecordCount > 0 Then
       Me.txtObjectNotSoldAmount = rs!counted
    Else
       Me.txtObjectNotSoldAmount = "N/A"
    End If
        
    
End Sub
