<div class="well col-md-4 col-xs-12 col-sm-12 object clickable" data-link="{baseurl url="Kavel/Item/`$object.id`"}">
    <img class="col-md-6 col-xs-6 col-sm-6" src="{$object.image}" alt="{$object.title}" />
    <div class="col-md-6 col-xs-6 col-sm-6">
        <div class="objectTitle">{$object.title}</div>
        <div class="objectTime countdown">{$object.timeRemaining}</div>
        <div class="objectBid"><h6>Huidig bod:</h6>&euro;{$object.currentBid}</div>
    </div>
</div>